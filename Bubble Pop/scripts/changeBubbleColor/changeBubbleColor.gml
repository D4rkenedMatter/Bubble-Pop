switch(global.bubbleTargetColor){
	case "red":
		image_blend = make_color_rgb(255,0,0);
		break;
	case "green":
		image_blend = make_color_rgb(0,255,0);
		break;
	case "blue":
		image_blend = make_color_rgb(0,0,255);
		break;
	case "yellow":
		image_blend = make_color_rgb(255,255,0);
		break;
	case "purple":
		image_blend = make_color_rgb(255,0,255);
		break;
	case "orange":
		image_blend = make_color_rgb(255,127,80);
		break;
	case "black":
		image_blend = make_color_rgb(0,0,0);
		break;
}